{
    "name": "tatum/utils",
    "description": "Utility functionality for all your WordPress plugins.",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "config": {
        "sort-packages": true,
        "platform": {
          "php": "7.4"
        }
    },
    "archive": {
        "exclude": [
            "lib",
            "tests"
        ]
    },
    "authors": [
        {
            "name": "Hathoriel"
        }
    ],
    "extra": {
        "clean-all-except": [
            "src",
            "dist",
            "dev",
            "languages"
        ]
    },
    "autoload": {
        "psr-4": {
            "Hathoriel\\Utils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hathoriel\\Utils\\Test\\": "test/phpunit/"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
      "10up/wp_mock": "^0.4.2",
      "antecedent/patchwork": "^2.1.12",
      "comcast/php-legal-licenses": "^1.1.2",
      "php-stubs/wordpress-stubs": "^5.3.2",
      "phpunit/phpunit": "^7"
    }
}
